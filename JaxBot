--[[
	
	AI LOGIC

 
 

]]-- 

function Walka()

	if cel.target ~= nil then
		Kombo()
	end	

end

function Kombo()
	
	if GetDistance(cel.target) < BRK.Zasieg then
		if BRK.Gotowe then
			CastSpell(BRK.Miejsce, cel.target)
		end
		if BC.Gotowe then
			CastSpell(BC.Miejsce, cel.target)
		end
	end
	
	if GetDistance(cel.target) <= CzarQ.Zasieg and CzarQ.Gotowe then
		if CzarW.Gotowe then
			CastSpell(_W)
		end
		CastSpell(_Q, cel.target)
	end
		
	if GetDistance(cel.target) <= CzarW.Zasieg+50 and CzarW.Gotowe then
		CastSpell(_W)
	end
		
	if GetDistance(cel.target) <= CzarE.Zasieg+50 and CzarE.Gotowe then
		CastSpell(_E)
	end
		
	if GetDistance(cel.target) <= CzarR.Zasieg and CzarR.Gotowe then
		CastSpell(_R)
	end
	
	if GetDistance(cel.target) <= AA.Zasieg + 150 then
		myHero:Attack(cel.target)
	end

end

function Gank()

end

function GraZDruzyna()

end

function Powrot()
	PierwszyPowrot = false
	if Strona == "Niebieska" then 
		myHero:MoveTo(BazaNiebieskich.x, BazaNiebieskich.z)
	else
		myHero:MoveTo(BazaCzerwonych.x, BazaCzerwonych.z)
	end
end

function AI_Mozg()
	
	if Strona == "Niebieska" then
		if math.sqrt((myHero.x - BazaNiebieskich.x) ^ 2 + (myHero.z - BazaNiebieskich.z) ^ 2) < BazaNiebieskich.zasieg and (czas_gry-CzasWSklepie >= NastepneKupowanie or czas_gry < 10) then
			--CzasWSklepie = czas_gry
			Status = "Sklep"
		end
	else
		if math.sqrt((myHero.x - BazaCzerwonych.x) ^ 2 + (myHero.z - BazaCzerwonych.z) ^ 2) < BazaCzerwonych.zasieg and (czas_gry-CzasWSklepie >= NastepneKupowanie or czas_gry < 10) then
			CzasWSklepie = czas_gry
			Status = "Sklep"
		end
	end
	
	if Status ~= "Sklep" or Status ~="Powrot" or Status ~= "Walka" then
		Jungle()
	end
	
	if Status == "Sklep" or myHero.dead then
		Sklep()
	end
	
	if Status == "Powrot" then
		Powrot()
	end
	
	if Status == "Walka" then
		Walka()
	end
	
	
	if PierwszyPowrot then
		if myHero.level >= 5 then
			PierwszyPowrot = false
		end
		if myHero.gold >= IloscKasyDoPowrotu1 and not WBazie() then
			Status = "Powrot"
		end
		if IloscZyciaDoPowrotu >= myHero.health/myHero.maxHealth and myHero.level > 2 and not WBazie() then
			Status = "Powrot"
		end
	else
		if IloscZyciaDoPowrotu >= myHero.health/myHero.maxHealth and myHero.level > 2 and not WBazie() then
			Status = "Powrot"
		end
		if myHero.gold >= IloscKasyDoPowrotu2 and not WBazie() then
			Status = "Powrot"
		end
	end
	

end

function WBazie()

	if Strona == "Niebieska" then
		if math.sqrt((myHero.x - BazaNiebieskich.x) ^ 2 + (myHero.z - BazaNiebieskich.z) ^ 2) < BazaNiebieskich.zasieg then
			return true
		else
			return false
		end
	else
		if math.sqrt((myHero.x - BazaCzerwonych.x) ^ 2 + (myHero.z - BazaCzerwonych.z) ^ 2) < BazaCzerwonych.zasieg then
			return true
		else
			return false
		end
	end

end


