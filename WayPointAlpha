--Gora
--Srodek
--Dol

function OnLoad()
	GankUstawienia = scriptConfig("Gank Helper", "Gank Helper")
	--BlueTeam
	--Top
	GankUstawienia:addParam("GoraDroga1","Draw 1st Way to gank Top", SCRIPT_PARAM_ONOFF, false)
	GankUstawienia:addParam("GoraDroga2","Draw 2st Way to gank Top", SCRIPT_PARAM_ONOFF, false)
	GankUstawienia:addParam("GoraDroga3","Draw 3st Way to gank Top", SCRIPT_PARAM_ONOFF, false)
	--Mid
	GankUstawienia:addParam("SrodekDroga1","Draw 1st Way to gank Mid", SCRIPT_PARAM_ONOFF, false)
	GankUstawienia:addParam("SrodekDroga2","Draw 2st Way to gank Mid", SCRIPT_PARAM_ONOFF, false)
	GankUstawienia:addParam("SrodekDroga3","Draw 3st Way to gank Mid", SCRIPT_PARAM_ONOFF, false)
	GankUstawienia:addParam("SrodekDroga4","Draw 3st Way to gank Mid", SCRIPT_PARAM_ONOFF, false)
	--Bot
	GankUstawienia:addParam("DolDroga1","Draw 1st Way to gank Bot", SCRIPT_PARAM_ONOFF, false)
	GankUstawienia:addParam("DolDroga2","Draw 2st Way to gank Bot", SCRIPT_PARAM_ONOFF, false)
	GankUstawienia:addParam("DolDroga3","Draw 3st Way to gank Bot", SCRIPT_PARAM_ONOFF, false)
	--RedTeam
	--Top
	--Mid
	--Bot
	
	--XYZ
	GankUstawienia:addParam("RysujXYZ","Draw XYZ Position", SCRIPT_PARAM_ONOFF, false)
end

