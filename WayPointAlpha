		DrawCircle(NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki2.PunkDrogi2X, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki2.PunkDrogi2Y, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki2.PunkDrogi2Z, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki2.Zasieg, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki2.Kolor)
		DrawCircle(NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki2.PunkDrogi3X, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki2.PunkDrogi3Y, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki2.PunkDrogi3Z, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki2.Zasieg, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki2.Kolor)
	end
	if GankUstawienia.DolDroga3 then
		DrawCircle(NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi1X, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi1Y, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi1Z, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.Zasieg, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.Kolor)
		DrawCircle(NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi2X, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi2Y, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi2Z, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.Zasieg, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.Kolor)
		DrawCircle(NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi3X, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi3Y, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi3Z, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.Zasieg, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.Kolor)
		DrawCircle(NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi4X, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi4Y, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi4Z, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.Zasieg, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.Kolor)
		DrawCircle(NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi5X, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi5Y, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.PunkDrogi5Z, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.Zasieg, NiebieskaDruzynaPunktDrogiDrogaDoDolnejDrozki3.Kolor)
	end
		
	if GankUstawienia.RysujXYZ then
 		DrawLine(100,175,200,175,1,0xFFFFFF00)
		DrawLine(100,175,100,260,1,0xFFFFFF00)
		DrawLine(200,175,200,260,1,0xFFFFFF00)
		DrawLine(100,260,200,260,1,0xFFFFFF00)
		
		DrawText("X: ".. math.floor(myHero.x),20,120,180,0xFF00FF00)
		DrawText("Y: ".. math.floor(myHero.y),20,120,210,0xFF00FF00)
		DrawText("Z: ".. math.floor(myHero.z),20,120,240,0xFF00FF00)
	end
end
