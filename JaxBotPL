		elseif obj.name == BlueCzerwonych.MlodyLizard1Nazwa then BlueCzerwonych.MlodyLizard1 = nil
		elseif obj.name == BlueCzerwonych.MlodyLizard2Nazwa then BlueCzerwonych.MlodyLizard2 = nil	
		elseif obj.name == WilkiCzerwonych.DuzyWilkNazwa then WilkiCzerwonych.DuzyWilk = nil
		elseif obj.name == WilkiCzerwonych.Wilk1Nazwa then WilkiCzerwonych.Wilk1 = nil
		elseif obj.name == WilkiCzerwonych.Wilk2Nazwa then WilkiCzerwonych.Wilk2 = nil
		elseif obj.name == DuchyCzerwonych.DuchNazwa then DuchyCzerwonych.Duch = nil
		elseif obj.name == DuchyCzerwonych.MalyDuch1Nazwa then DuchyCzerwonych.MalyDuch1 = nil
		elseif obj.name == DuchyCzerwonych.MalyDuch2Nazwa then DuchyCzerwonych.MalyDuch2 = nil
		elseif obj.name == DuchyCzerwonych.MalyDuch3Nazwa then DuchyCzerwonych.MalyDuch3 = nil
		elseif obj.name == RedCzerwonych.StarszyJaszczurNazwa then RedCzerwonych.StarszyJaszczur = nil
		elseif obj.name == RedCzerwonych.MlodyJaszczur1Nazwa then RedCzerwonych.MlodyJaszczur1 = nil
		elseif obj.name == RedCzerwonych.MlodyJaszczur2Nazwa then RedCzerwonych.MlodyJaszczur2 = nil
		elseif obj.name == GolemyCzerwonych.GolemNazwa then GolemyCzerwonych.Golem = nil
		elseif obj.name == GolemyCzerwonych.MalyGolemNazwa then GolemyCzerwonych.MalyGolem = nil
		-- Smok
		elseif obj.name == Smok.SmokNazwa then Smok.Smok = nil
		-- Baron
		elseif obj.name == Baron.BaronNazwa then Baron.Baron = nil
		end	
	end
end

function ZaladowaniePotworowWJungle()
        for i = 1, objManager.maxObjects do
			local obj = objManager:getObject(i)
            if obj ~= nil and obj.type == "obj_AI_Minion" and obj.name ~= nil then
				--Niebiescy
				if obj.name == 	BlueNiebieskich.StarozytnyGolemNazwa then BlueNiebieskich.StarozytnyGolem = obj
				elseif obj.name == BlueNiebieskich.MlodyLizard1Nazwa then BlueNiebieskich.MlodyLizard1 = obj
				elseif obj.name == BlueNiebieskich.MlodyLizard2Nazwa then BlueNiebieskich.MlodyLizard2 = obj	
				elseif obj.name == WilkiNiebieskich.DuzyWilkNazwa then WilkiNiebieskich.DuzyWilk = obj
				elseif obj.name == WilkiNiebieskich.Wilk1Nazwa then WilkiNiebieskich.Wilk1 = obj
				elseif obj.name == WilkiNiebieskich.Wilk2Nazwa then WilkiNiebieskich.Wilk2 = obj
				elseif obj.name == DuchyNiebieskich.DuchNazwa then DuchyNiebieskich.Duch = obj
				elseif obj.name == DuchyNiebieskich.MalyDuch1Nazwa then DuchyNiebieskich.MalyDuch1 = obj
				elseif obj.name == DuchyNiebieskich.MalyDuch2Nazwa then DuchyNiebieskich.MalyDuch2 = obj
				elseif obj.name == DuchyNiebieskich.MalyDuch3Nazwa then DuchyNiebieskich.MalyDuch3 = obj
				elseif obj.name == RedNiebieskich.StarszyJaszczurNazwa then RedNiebieskich.StarszyJaszczur = obj
				elseif obj.name == RedNiebieskich.MlodyJaszczur1Nazwa then RedNiebieskich.MlodyJaszczur1 = obj
				elseif obj.name == RedNiebieskich.MlodyJaszczur2Nazwa then RedNiebieskich.MlodyJaszczur2 = obj
				elseif obj.name == GolemyNiebieskich.GolemNazwa then GolemyNiebieskich.Golem = obj
				elseif obj.name == GolemyNiebieskich.MalyGolemNazwa then GolemyNiebieskich.MalyGolem = obj
				-- Czerwoni
				elseif obj.name == 	BlueCzerwonych.StarozytnyGolemNazwa then BlueCzerwonych.StarozytnyGolem = obj
				elseif obj.name == BlueCzerwonych.MlodyLizard1Nazwa then BlueCzerwonych.MlodyLizard1 = obj
				elseif obj.name == BlueCzerwonych.MlodyLizard2Nazwa then BlueCzerwonych.MlodyLizard2 = obj	
				elseif obj.name == WilkiCzerwonych.DuzyWilkNazwa then WilkiCzerwonych.DuzyWilk = obj
				elseif obj.name == WilkiCzerwonych.Wilk1Nazwa then WilkiCzerwonych.Wilk1 = obj
				elseif obj.name == WilkiCzerwonych.Wilk2Nazwa then WilkiCzerwonych.Wilk2 = obj
				elseif obj.name == DuchyCzerwonych.DuchNazwa then DuchyCzerwonych.Duch = obj
				elseif obj.name == DuchyCzerwonych.MalyDuch1Nazwa then DuchyCzerwonych.MalyDuch1 = obj
				elseif obj.name == DuchyCzerwonych.MalyDuch2Nazwa then DuchyCzerwonych.MalyDuch2 = obj
				elseif obj.name == DuchyCzerwonych.MalyDuch3Nazwa then DuchyCzerwonych.MalyDuch3 = obj
				elseif obj.name == RedCzerwonych.StarszyJaszczurNazwa then RedCzerwonych.StarszyJaszczur = obj
				elseif obj.name == RedCzerwonych.MlodyJaszczur1Nazwa then RedCzerwonych.MlodyJaszczur1 = obj
				elseif obj.name == RedCzerwonych.MlodyJaszczur2Nazwa then RedCzerwonych.MlodyJaszczur2 = obj
				elseif obj.name == GolemyCzerwonych.GolemNazwa then GolemyCzerwonych.Golem = obj
				elseif obj.name == GolemyCzerwonych.MalyGolemNazwa then GolemyCzerwonych.MalyGolem = obj
				-- Smok
				elseif obj.name == Smok.SmokNazwa then Smok.Smok = obj
				-- Baron
				elseif obj.name == Baron.BaronNazwa then Baron.Baron = obj
                end
			end
        end
end

function PotworyOdrodzenie()

	if czas_gry <= WilkiNiebieskich.Pierwsze_odrodzenie + 5 then
		if czas_gry >= BlueNiebieskich.Pierwsze_odrodzenie then
			BlueNiebieskich.Zyje = true
			RedNiebieskich.Zyje = true
			
			BlueCzerwonych.Zyje = true
			RedCzerwonych.Zyje = true
		end
		
		if czas_gry >= WilkiNiebieskich.Pierwsze_odrodzenie then
			WilkiNiebieskich.Zyje = true
			DuchyNiebieskich.Zyje = true
			GolemyNiebieskich.Zyje = true
			
			WilkiCzerwonych.Zyje = true
			DuchyCzerwonych.Zyje = true
			GolemyCzerwonych.Zyje = true
			PierwszeOdrodzenie = false
		end
	
	else
		if czas_gry - WilkiNiebieskich.Czas_smierci >= WilkiNiebieskich.Czas_odrodzenia and WilkiNiebieskich.Zyje == false or (WilkiNiebieskich.DuzyWilk ~= nil or WilkiNiebieskich.Wilk1 ~= nil or WilkiNiebieskich.Wilk2 ~= nil) then WilkiNiebieskich.Zyje = true end
		if czas_gry - DuchyNiebieskich.Czas_smierci >= DuchyNiebieskich.Czas_odrodzenia and DuchyNiebieskich.Zyje == false or (DuchyNiebieskich.Duch ~= nil or DuchyNiebieskich.MalyDuch1 ~= nil or DuchyNiebieskich.MalyDuch2 ~= nil or DuchyNiebieskich.MalyDuch3 ~= nil) then DuchyNiebieskich.Zyje = true end
		if czas_gry - GolemyNiebieskich.Czas_smierci >= GolemyNiebieskich.Czas_odrodzenia and GolemyNiebieskich.Zyje == false or (GolemyNiebieskich.Golem ~= nil or GolemyNiebieskich.MalyGolem ~= nil) then GolemyNiebieskich.Zyje = true end
		if czas_gry - BlueNiebieskich.Czas_smierci >= BlueNiebieskich.Czas_odrodzenia and BlueNiebieskich.Zyje == false or (BlueNiebieskich.StarozytnyGolem ~= nil or BlueNiebieskich.MlodyLizard1 ~= nil or BlueNiebieskich.MlodyLizard2 ~= nil) then BlueNiebieskich.Zyje = true end
		if czas_gry - RedNiebieskich.Czas_smierci >= RedNiebieskich.Czas_odrodzenia and RedNiebieskich.Zyje == false or (RedNiebieskich.StarszyJaszczur ~= nil or RedNiebieskich.MlodyJaszczur1 ~= nil or RedNiebieskich.MlodyJaszczur2 ~= nil) then RedNiebieskich.Zyje = true end
		
		if czas_gry - WilkiCzerwonych.Czas_smierci >= WilkiCzerwonych.Czas_odrodzenia and WilkiCzerwonych.Zyje == false or (WilkiCzerwonych.DuzyWilk ~= nil or WilkiCzerwonych.Wilk1 ~= nil or WilkiCzerwonych.Wilk2 ~= nil) then WilkiCzerwonych.Zyje = true end
		if czas_gry - DuchyCzerwonych.Czas_smierci >= DuchyCzerwonych.Czas_odrodzenia and DuchyCzerwonych.Zyje == false or (DuchyCzerwonych.Duch ~= nil or DuchyCzerwonych.MalyDuch1 ~= nil or DuchyCzerwonych.MalyDuch2 ~= nil or DuchyCzerwonych.MalyDuch3 ~= nil) then DuchyCzerwonych.Zyje = true end
		if czas_gry - GolemyCzerwonych.Czas_smierci >= GolemyCzerwonych.Czas_odrodzenia and GolemyCzerwonych.Zyje == false or (GolemyCzerwonych.Golem ~= nil or GolemyCzerwonych.MalyGolem ~= nil) then GolemyCzerwonych.Zyje = true end
		if czas_gry - BlueCzerwonych.Czas_smierci >= BlueCzerwonych.Czas_odrodzenia and BlueCzerwonych.Zyje == false or (BlueCzerwonych.StarozytnyGolem ~= nil or BlueCzerwonych.MlodyLizard1 ~= nil or BlueCzerwonych.MlodyLizard2 ~= nil) then BlueCzerwonych.Zyje = true end
		if czas_gry - RedCzerwonych.Czas_smierci >= RedCzerwonych.Czas_odrodzenia and RedCzerwonych.Zyje == false or (RedCzerwonych.StarszyJaszczur ~= nil or RedCzerwonych.MlodyJaszczur1 ~= nil or RedCzerwonych.MlodyJaszczur2 ~= nil) then RedCzerwonych.Zyje = true end
	end
	
	if PierwszeOdrodzenieSmoka then
		if czas_gry >= Smok.Pierwsze_odrodzenie then
			Smok.Zyje = true
			PierwszeOdrodzenieSmoka = false
		end
	else
		if czas_gry - Smok.Czas_smierci >= Smok.Czas_odrodzenia and Smok.Zyje == false then Smok.Zyje = true end
	end
	
	if PierwszeOdrodzenieBarona then
		if czas_gry >= Baron.Pierwsze_odrodzenie then
			Baron.Zyje = true
			PierwszeOdrodzenieBarona = false
		end
	else
		if czas_gry - Baron.Czas_smierci >= Baron.Czas_odrodzenia and Baron.Zyje == false then Baron.Zyje = true end
	end

end
